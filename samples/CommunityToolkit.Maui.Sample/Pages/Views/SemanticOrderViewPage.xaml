<?xml version="1.0" encoding="utf-8" ?>
<pages:BasePage
	x:Class="CommunityToolkit.Maui.Sample.Pages.Views.SemanticOrderViewPage"
	xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	xmlns:mct="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
	xmlns:pages="clr-namespace:CommunityToolkit.Maui.Sample.Pages"
	xmlns:viewModels="clr-namespace:CommunityToolkit.Maui.Sample.ViewModels.Views"
	Title="SemanticOrderView"
	x:DataType="viewModels:SemanticOrderViewViewModel"
	x:TypeArguments="viewModels:SemanticOrderViewViewModel">
	<mct:SemanticOrderView x:Name="acv">
		<StackLayout BackgroundColor="White" HeightRequest="500">
			<Label
				x:Name="second"
				Margin="0,20"
				Text="Second" />
			<Button
				x:Name="third"
				Margin="0,20"
				Text="Third" />
			<Label
				x:Name="fourth"
				Margin="0,20"
				Text="Fourth" />
			<Button
				x:Name="fifth"
				Margin="0,20"
				Text="Fifth and last" />
			<Button
				x:Name="first"
				Margin="0,20"
				Text="First" />
		</StackLayout>
	</mct:SemanticOrderView>
</pages:BasePage>